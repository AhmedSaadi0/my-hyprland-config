function fish_greeting
    set variables $ascii_1 $ascii_2 $ascii_3 $ascii_4 $ascii_5 $ascii_6 $ascii_7 $ascii_8 $ascii_9 $ascii_10 $ascii_11 $ascii_12 $ascii_13 $ascii_14 $ascii_16 $ascii_15 $ascii_17

    set random_index (shuf -i 1-17 -n 1)

    set file_path ".cache/ascii-fish.temp"

    # Check if the file exists
    if test -e $file_path
        # File exists
        set current_number (cat $file_path)

        # Check if the content is a number
        if not string match -qr '^[0-9]+$' $current_number
            # If not a number, clean the file and write 0
            echo 0 > $file_path
        else
            # Increment the number by 1
            set current_number (math "$current_number + 1")

            # Check if the number is greater than or equal to 16
            if test $current_number -ge 17
                # If greater than or equal to 16, reset to 0
                echo 0 > $file_path
            else
                # Write the incremented number back to the file
                echo $current_number > $file_path
            end
        end
    else
        # File does not exist, create it and write 0
        echo 0 > $file_path
    end

    set random_variable $variables[$current_number]

    echo -e $random_variable
    # echo -e $ascii_18
end

set -x EDITOR /usr/bin/nano
